        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "pluginVersion": "9.5.3",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "editorMode": "code",
          "expr": "count((100 * (sum by(job) (rate(ac_errors_total{job=\"ac-service\"}[5m])) / (sum by(job) (rate(ac_token_issuance_total{job=\"ac-service\"}[5m])) + sum by(job) (rate(ac_jwks_requests_total{job=\"ac-service\"}[5m])))) > 1) or (100 * (sum by(job) (rate(gc_http_requests_total{job=\"gc-service\", status_code=~\"[45]..\"}[5m])) / sum by(job) (rate(gc_http_requests_total{job=\"gc-service\"}[5m]))) > 1) or (100 * (sum by(job) (rate(mc_messages_dropped_total{job=\"mc-service\"}[5m])) / (sum by(job) (rate(mc_messages_dropped_total{job=\"mc-service\"}[5m])) + sum by(job) (rate(mc_message_latency_seconds_count{job=\"mc-service\"}[5m])))) > 1)) or vector(0)",
          "legendFormat": "Services Violating SLO",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "SLO Violations (Count)",
      "type": "stat"
    },
